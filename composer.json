{
    "name": "antonmarin/codeception-openapi",
    "description": "Module for codeception to test server over OpenAPI specification",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Marin",
            "email": "antonaryo85@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "antonmarin\\CodeceptionOpenApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "antonmarin\\CodeceptionOpenApiTests\\": ["tests/", "tests/_support/"]
        }
    },
    "require": {
        "php": "^7.1",
        "codeception/codeception": "^3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12",
        "codeception/util-universalframework": "^1.0"
    }
}
